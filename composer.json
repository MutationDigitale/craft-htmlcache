{
    "name": "mutation/htmlcache",
    "description": "Html static cache.",
    "version": "1.0.1",
    "type": "craft-plugin",
    "support": {
        "docs": "https://github.com/MutationDigitale/craft3-htmlcache/blob/master/README.md",
        "issues": "https://github.com/MutationDigitale/craft3-htmlcache/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mutation Digitale",
            "homepage": "https://mutation.io/"
        }
    ],
    "require": {
        "php": ">7.1",
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "mutation\\htmlcache\\": "src/"
        }
    },
    "extra": {
        "name": "HTML Cache",
        "handle": "html-cache",
        "hasCpSettings": true,
        "hasCpSection": false,
        "schemaVersion": "1.0.0",
        "changelogUrl": "https://raw.githubusercontent.com/MutationDigitale/craft3-htmlcache/master/CHANGELOG.md",
        "class": "mutation\\htmlcache\\HtmlCache"
    }
}